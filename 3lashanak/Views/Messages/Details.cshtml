@model _3lashanak.Models.Messages

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<div>
    <h4>Messages</h4>
    <hr />
    <dl class="row">
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Id)
        </dd>
        <dt class = "col-sm-2">
            الاسم
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Name)
        </dd>
        <dt class = "col-sm-2">
            الايميل
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Email)
        </dd>
        <dt class = "col-sm-2">
            رقم الهاتف
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.PhoneNumber)
        </dd>
        <dt class = "col-sm-2">
            الموضوع
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Subject)
        </dd>
        <dt class = "col-sm-2">
            الرسالة
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Body)
        </dd>
        <dt class = "col-sm-2">
            تاريخ الارسال
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.SendDate)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Index">عودة للقائمة الرئيسية</a>
</div>
