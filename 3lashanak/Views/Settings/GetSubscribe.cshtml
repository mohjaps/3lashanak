@model IEnumerable<_3lashanak.Models.Settings>



<h1>طلبات الاشترااك</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>

            <th>
                @Html.DisplayNameFor(model => model.Value)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Value)
            </td>

            <td>
                <a class="btn btn-outline-primary" asp-action="Edit" asp-route-id="@item.Id">تعديل</a> |
                <a class="btn btn-outline-primary" asp-action="Details" asp-route-id="@item.Id">تفاصيل</a> |
                <a class="btn btn-outline-danger" asp-action="Delete" asp-route-id="@item.Id">حذف</a>
            </td>
        </tr>
}
    </tbody>
</table>
